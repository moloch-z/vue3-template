{
  "name": "vue3-template",
  "version": "1.0.0",
  "description": "vue3 + typescript template",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm -y",
    "prepare": "husky install",
    "start": "npm i -g pnpm && pnpm i & pnpm dev",
    "dev": "vite -c ./vite.config.dev.ts --mode development",
    "build": "vite build -c ./vite.config.prod.ts --mode production",
    "report": "vite build -c ./vite.config.prod.ts --mode production",
    "lint:es": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "lint:css": "stylelint --cache src/**/*.{css,sass,scss,less} --fix",
    "am": "git add . & git-cz",
    "cm": "git-cz",
    "test": "jest",
    "test-c": "jest --coverage",
    "e2e": "node ./__tests__/e2e/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.22.7",
    "@types/jest": "^29.5.2",
    "@types/three": "^0.149.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/runtime-core": "^3.2.47",
    "@vue/test-utils": "^2.4.0",
    "@vue/vue3-jest": "28.x",
    "babel-jest": "^29.6.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-conventional-changelog-zh": "^0.0.2",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-jsdoc": "^40.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^29.6.1",
    "less": "^4.1.3",
    "lint-staged": "^13.2.0",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.4",
    "rollup-plugin-external-globals": "^0.7.3",
    "rollup-plugin-visualizer": "^5.9.0",
    "selenium-webdriver": "^4.10.0",
    "stylelint": "^15.3.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-less": "^1.0.4",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-config-standard": "^31.0.0",
    "stylelint-less": "^1.0.6",
    "stylelint-scss": "^4.5.0",
    "ts-jest": "^29.1.1",
    "ts-jest-mock-import-meta": "^1.0.0",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vue-eslint-parser": "^9.1.0"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^3.2.15",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "pinia": "^2.0.33",
    "three": "^0.150.1",
    "vue": "^3.2.47",
    "vue-demi": "^0.13.11",
    "vue-dompurify-html": "^4.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "lint-staged": {
    "/src/**/*.{js,jsx,ts,tsx,vue}": [
      "eslint --cache --fix",
      "prettier --write",
      "git add"
    ],
    "/src/**/*.{css,scss,less}": [
      "stylelint --cache --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.md": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-zh"
    }
  }
}
